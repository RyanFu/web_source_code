}

a,a:link,a:visited,a:active,.faux_link{text-decoration: none;color:#2786c2;outline: none;cursor: pointer;}a:hover,.faux_hover,.faux_link:hover{
    color:#f75342;
    text-decoration: none;

}
#roundrect_cap {
    position:absolute;
    width: 980px;
    height: 40px;
    top: 83px;
    left: -1px;
    z-index: 1;
}
#cap {
    -webkit-border-top-left-radius:20px;
    -webkit-border-top-right-radius:20px;
    -moz-border-radius-topleft:20px;
    -moz-border-radius-topright:20px;
    height: 20px;
    position: relative;
    z-index: 0;
    background-color: #FFF;
    *background-color:transparent;


}
.pagination {
    padding:10px 0 0;
    margin: 10px 0px 10px 10px;
    *margin: 0px 0px 0px 10px; 
}
.pagination li.selected {
    color:#3E3E3E;
    font:bold 24px arial,sans-serif;
    margin:-6px 0 0;
    padding:2px 6px;
    position:relative;
}
.pagination li a {
    border:2px solid #EDF1F5;
    color:#969696;
    display:block;
    padding:0 3px;
}
.pagination li.selected a {
    border:none;
    color:#3E3E3E;
    padding: 0px;
}
.pagination li.dots {
    color:#CCCCCC;
    font-weight:bold;
}
.pagination li a:hover{color: white;background:#59a5d1;border:2px solid #408bb6;}
.pagination li.selected a:hover{color: #3E3E3E;background:none;border:none;}

.pagination li.arrow,.pagination li.arrow a,.pagination li.arrow a:hover{margin:0;border:0;padding:0;background:#fff;}.pagination li {
    display:block;
    float:left;
    font:14px arial,sans-serif;
    margin:0 4px;
}
a.next {	
    height:33px;
    text-align: center;
    color: #2786c2;
    vertical-align: middle;
    line-height: 33px;
    border: 1px solid #EDF1F5;
    cursor: pointer;
    display: block;
    font-size: 18px;
    margin:10px 10px 0px 10px;
    *margin:0px 10px 0px 10px;
}

a.next:hover  {

    *margin:0px 10px 0px 10px;
    height:33px;
    text-align: center;
    background-color: #59A5D1;
    color: #fff;
    vertical-align: middle;
    line-height: 33px;
    border: 1px solid #408bb6;
    cursor: pointer;
}

