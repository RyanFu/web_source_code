.brozar .tabs .softcorner{ -webkit-border-radius:8px 8px 0px 0px;  -moz-border-radius-topleft: 8px; -moz-border-radius-topright: 8px;float:left;font: normal 16px arial,sans-serif;margin-right:5px;*margin-right:0px;color:#3e3e3e;padding:8px 12px 4px 12px;cursor: pointer;}

.brozar .more,.brozar .add{
    -moz-border-radius-topright: 15px; -moz-border-radius-bottomright: 15px; -moz-border-radius-bottomleft: 15px;
