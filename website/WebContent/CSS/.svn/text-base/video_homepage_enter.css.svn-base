.columnAB{
    margin:0px 0px 0px 20px;
}
.comments{list-style-type: none;margin:0;padding:0;}.comments li{
    margin:0;
    padding:10px 0px;
    font: normal 12px/17px verdana,sans-serif;
}.comments li.first{
    background: transparent !important;
    border: none;
}.comments li img.portrait{float: left;}.comments li div.rightside{
    float: left;
    margin-left:10px;
}.comments li.reply,.comments li.reply_controls{background: transparent !important;}.comments li.reply img.portrait{width:50px;height:50px;}.comments li.reply{margin:0 0 5px 25px !important;padding:0 0 1px 0 !important;font: normal 10px/14px Verdana,sans-serif;}.comments li.reply div.rightside{
    padding:10px;
}.comments li div.name,.new_comment .isay{
    color:#969696;
    font: normal 12px/12px verdana,sans-serif;
    margin:0 0 5px 0;
}.new_comment .isay strong{
}.comments li div.name a{
    font-weight:bold;
    font-size:12px;
}.comments li div.date,.new_comment .nohtml,.reply_controls .nohtml{color:#9b9999;font-size:10px;margin:0 0 0 5px;}.comments li div.text{
    font: normal 12px/18px verdana,sans-serif;
    margin:5px 0 10px 0;
}.comments li div.version{color:#97A500;font: normal 10px verdana,serif;float: right;}.comments li div.text div{padding-bottom:5px;}.comments li div.replylink{
    font-size:12px;
}.comments li.reply_controls{margin:0px 0px 7px 85px;padding:0;}.comments li textarea,.comments li.reply textarea,.new_comment textarea{height:75px;font: normal 11px verdana,sans-serif;margin-bottom:10px;padding:5px;border:1px solid #eaeaea;}.portrait{cursor:pointer;}

.columnAB .parent {
