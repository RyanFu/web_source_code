.brozar .scrolly_container{-moz-border-radius: 10px 10px 10px 10px;-webkit-border-radius:10px;padding:10px;*padding:0px 10px;position: relative;}
.scrolly_area .content{overflow: hidden;float: left;}
.brozar .content .clip{padding-bottom:10px;}
