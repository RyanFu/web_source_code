#main .columns{padding-left:20px;padding-right:20px;width:940px;}#main .column{
    float: left;
    color:#3e3e3e;
    font-size:12px;
}#main #columnA{width:160px;margin-top:20px;margin-right:10px;}#main #columnB{width:440px;margin-top:20px;margin-right:10px;margin-left:10px;}#main #columnC{width:300px;margin-top:20px;margin-left:10px;font: normal 12px/14px verdana;color:#3e3e3e;}#settings_options li{
    display:block;
    width:152px;
    height:19px;
    background:#f2f2ee;
    margin-bottom:1px;
    padding-top:6px;
    padding-left:8px;
    line-height:12px;
    cursor: pointer;
}#settings_options li.current{background:#34b0ff;margin:5px 0px;}#settings_options li.delete{background:#fcc;color:#f75342;margin-top:10px;}#settings_options li.delete span{line-height:10px;}#settings_options li.current a{color:#fff;font-weight:bold;}#settings_options li img.new{float: right;margin-right:5px;}#columnB li{margin:0px 0px 10px 0px;}textarea{border:3px solid #deded0;font:Normal 12px Verdana;color:#3e3e3e;padding:3px;width:400px;height:50px;margin:3px 0px 0px 0px;overflow:hidden;}input{border:3px solid #deded0;font:Normal 12px Verdana;color:#3e3e3e;padding:3px;margin:3px 0px 0px 0px;}
.blue_button{
    cursor: pointer;
    background:#59a5d1;
    font:12px verdana,sans-serif;
    color:#fff ;
    border:1px solid #408bb6;
    padding:2px 10px;
}.dark_blue_button{cursor: pointer;background:#2498CC;font:12px verdana,sans-serif;color:#fff !important;border:1px solid #2498CC;padding:2px 10px;}.orange_button{cursor: pointer;background:#f75342;font:12px verdana,sans-serif;color:#fff !important;padding:2px 10px;border:1px solid #ca4a3c;}.green_button{cursor: pointer;background:#93c117;font:12px verdana,sans-serif;color:#fff !important;padding:2px 10px;border:1px solid #83b117;}.grey_button{cursor:default;background:#F2F2EE;font:12px verdana,sans-serif;color:#969696 !important;padding:2px 10px;border:1px solid #DEDED0;}.dark_grey_button{cursor: pointer;background:#3e3e3e;font:12px verdana,sans-serif;color:#fff !important;padding:2px 10px;border:1px solid #2e2e2e;}
ul.password input {
    border:3px solid #DEDED0;
    color:#3E3E3E;
    font:12px Verdana;
    margin:3px 0 10px;
    padding:3px;
    width:250px;
}
.settingsLable {font-size:18px;font-weight:bold;}
#settings_menu li {
    background:none repeat scroll 0 0 #F2F2EE;
    display:block;
    height:19px;
    line-height:11px;
    margin-bottom:1px;
    padding-left:8px;
    padding-top:6px;
    width:152px;
}
#settings_menu li.current {
    background:none repeat scroll 0 0 #34B0FF;
    color:#FFFFFF;
    font-weight:bold;
    margin:5px 0;
}
input#save_btn, #goto_alb_btn {
    font:18px arial,sans-serif;
    width:160px;
}
#info_area input {
width:350px;
}

div.ddp{
    margin-bottom: 20px;
}

div.ddp div.dp_wrapper {
    float: left;
    margin: 10px 10px 0px 0px;
    cursor: pointer;
    height: 53px;
    width: 53px;
}