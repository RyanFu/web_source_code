#stats{margin-bottom:20px;}#stats #graph{margin:10px 0 5px 0;}#stats #graph ul li{display:block;float: left;width:93px;margin:0 10px 0 0;background:none;}#stats #graph ul li.last{margin:0;}#stats #graph ul li p{font:bold 11px/11px arial,sans-serif;text-align:center;margin: -1px 0 0 0;padding:10px 0;}#stats #graph ul li.plays p{background:#d9f2e5;color:#6c9;}#stats #graph ul li.likes p{background:#fed5d7;color:#fc575e;}#stats #graph ul li.comments p{background:#d3eeff;color:#4ebaff;}#stats #graph ul li p var{display:block;font:bold 24px/28px arial,sans-serif;letter-spacing: -1px;}#stats #graph ul li div.sparkline{margin:0;width:93px;height:20px;}#stats #graph p.date{color:#969696;font: normal 10px verdana,sans-serif;text-align:center;margin:10px 0 0 0;}#stats table.spreadsheet{display:block;margin:10px 0 5px 0;width:300px;font:bold 12px/12px arial,sans-serif;border-collapse:separate;/border-collapse:collapse;}#stats table.spreadsheet thead th{font-weight:bold;text-align:center;vertical-align:bottom;padding:3px;width:76px;font-size:12px;border-bottom:3px solid #ddd;}#stats table.spreadsheet thead th.date{width:72px;color:#969696;}#stats table.spreadsheet thead th.total_plays{color:#6c9;border-bottom-color:#6c9;}#stats table.spreadsheet thead th.likes{color:#fc575e;border-bottom-color:#fc575e;}#stats table.spreadsheet thead th.comments{color:#4ebaff;border-bottom-color:#4ebaff;}#stats table.spreadsheet tr td{font-size:14px;text-align: right;padding:5px 10px;border-bottom:1px dotted #ddd;}#stats table.spreadsheet tfoot tr td,#stats table.spreadsheet tfoot tr th{border-top:3px solid #ddd !important;border-bottom: none;}#stats table.spreadsheet tbody tr td.grey{color:#969696;}#stats table.spreadsheet tbody tr.alt td{}#stats table.spreadsheet tbody tr td.first{border-left:1px dotted #ddd;}#stats table.spreadsheet tbody tr td.date{
    font: normal 12px verdana,sans-serif;
