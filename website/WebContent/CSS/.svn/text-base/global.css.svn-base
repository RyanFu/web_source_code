}#stats table.spreadsheet tbody tr td.date sup{font-size:9px;vertical-align: text-top;}#stats table.spreadsheet tfoot tr td,#stats table.spreadsheet tfoot tr th{padding:4px 10px 0 10px;text-align:right;font-size:14px;border-top:1px dotted #ddd;}#stats table.spreadsheet tfoot tr th{font-size:12px;}#stats table.spreadsheet tfoot tr th{text-align:center;}#stats #stats_pager{
    font: normal 12px verdana,sans-serif;
