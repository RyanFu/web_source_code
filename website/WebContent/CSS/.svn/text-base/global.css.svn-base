}#stats #hd_embed_stats{margin:10px 0 5px 0;padding:5px 10px 5px 40px;font:normal 10px/8px verdana,sans-serif;border:1px dotted #ecb7be;background:#fcc7ce  no-repeat 10px center;}#stats #hd_embed_stats var{font:bold 12px arial,sans-serif;}#stats #embed_stats p{margin-top:5px;}#stats #embed_stats ul{margin-top:5px;}#stats #embed_stats ul li a{font-weight:bold;}#stats #embed_stats ul li a.blacklist{color:#ccc;}#stats #embed_stats ul li a.blacklisted{color:#f00;}
#stats #graph{margin:10px 0 5px 0;}#stats #graph ul li{display:block;float: left;width:93px;margin:0 10px 0 0;background:none;}#stats #graph ul li.last{margin:0;}#stats #graph ul li p{font:bold 11px/11px arial,sans-serif;text-align:center;margin: -1px 0 0 0;padding:10px 0;}#stats #graph ul li.plays p{background:#d9f2e5;color:#6c9;}#stats #graph ul li.likes p{background:#fed5d7;color:#fc575e;}#stats #graph ul li.comments p{background:#d3eeff;color:#4ebaff;}#stats #graph ul li p var{display:block;font:bold 24px/28px arial,sans-serif;letter-spacing: -1px;}#stats #graph ul li div.sparkline{margin:0;width:93px;height:20px;}#stats #graph p.date{color:#969696;font: normal 10px verdana,sans-serif;text-align:center;margin:10px 0 0 0;}
